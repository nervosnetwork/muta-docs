alue === 'string' && humanize) ? humanizeString(value) : value;\n\n        return (\n          <label key={idx}>\n            <input\n              checked={value == currentState}\n              name={name}\n              onChange={(event) => setState(value)}\n              type=\"radio\"\n               />\n            {label && <>{icon ? <i className={`feather icon-${icon}`}></i> : ''} {label}</>}\n          </label>\n        );\n      })}\n    </>\n  );\n}\n\nexport default RadioList;\n"]}]}