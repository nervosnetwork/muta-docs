,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '开发概览'\n};\nexport const metadata = {\n  \"id\": \"docs_zh/dev/dev-overview\",\n  \"title\": \"开发概览\",\n  \"description\": \"在该章节，我们将为你详细介绍如何通过开发 Service 对链的功能进行定制。Service 是 Muta 框架中用于扩展的抽象层，每个 Service 完成一个相对独立的功能，单独维护自己的存储和操作接口，类似一个运行在沙盒里的小型状态机，这些 Service 共同组成了链的状态机部分，通过框架接口将状态机接入区块链底层组件，一条专属你的全新链就开发完成了！\",\n  \"source\": \"@site/docs/docs_zh/dev/dev-overview.md\",\n  \"permalink\": \"/new-docs/docs/docs_zh/dev/dev-overview\",\n  \"editUrl\": \"https://github.com/nervosnetwork/muta/edit/master/website/docs/docs_zh/dev/dev-overview.md\",\n  \"sidebar\": \"docs_zh\",\n  \"previous\": {\n    \"title\": \"多节点部署指南\",\n    \"permalink\": \"/new-docs/docs/docs_zh/setup/deploy\"\n  },\n  \"next\": {\n    \"title\": \"Service 开发指南\",\n    \"permalink\": \"/new-docs/docs/docs_zh/dev/service-dev\"\n  }\n};\n  /* @jsx mdx */\nimport Step from '@site/src/components/Step';\nimport Jump from '@site/src/components/Jump';\nexport const rightToc = [{\n  value: '学习路径',\n  id: '学习路径',\n  children: [{\n    value: '阅读 Service 开发指南',\n    id: '阅读-service-开发指南',\n    children: []\n  }, {\n    value: '参考目前已有的 Service，进一步学习如何开发 Service',\n    id: '参考目前已有的-service，进一步学习如何开发-service',\n    children: []\n  }, {\n    value: '尝试开发一个简单 attestation（存证）Service，搭建一条存证链',\n    id: '尝试开发一个简单-attestation（存证）service，搭建一条存证链',\n    children: []\n  }, {\n    value: '学习开发更为复杂的应用 - 去中心化交易所',\n    id: '学习开发更为复杂的应用---去中心化交易所',\n    children: []\n  }]\n}, {\n  value: '内置 Service 说明',\n  id: '内置-service-说明',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <p>{`在该章节，我们将为你详细介绍如何通过开发 Service 对链的功能进行定制。Service 是 Muta 框架中用于扩展的抽象层，每个 Service 完成一个相对独立的功能，单独维护自己的存储和操作接口，类似一个运行在沙盒里的小型状态机，这些 Service 共同组成了链的状态机部分，通过框架接口将状态机接入区块链底层组件，一条专属你的全新链就开发完成了！`}</p>\n    <h2 {...{\n      \"id\": \"学习路径\"\n    }}>{`学习路径`}</h2>\n    <Step headingDepth={3} mdxType=\"Step\">\n      <ol>\n        <li>\n          <h3 {...{\n            \"id\": \"阅读-service-开发指南\"\n          }}>{`阅读 Service 开发指南`}</h3>\n          <p>{`该文档主要介绍了 Service 的组成和开发指南`}</p>\n          <Jump to=\"../service-dev/\" mdxType=\"Jump\">View Service Dev Guide</Jump>\n        </li>\n        <li>\n          <h3 {...{\n            \"id\": \"参考目前已有的-service，进一步学习如何开发-service\"\n          }}>{`参考目前已有的 Service，进一步学习如何开发 Service`}</h3>\n          <p>{`我们提供了许多通用的 Service 可供参考和使用，涉及治理，资产，多签等等。`}</p>\n          <Jump to=\"../../dev/service-list/\" mdxType=\"Jump\">View Service List</Jump>\n        </li>\n        <li>\n          <h3 {...{\n            \"id\": \"尝试开发一个简单-attestation（存证）service，搭建一条存证链\"\n          }}>{`尝试开发一个简单 attestation（存证）Service，搭建一条存证链`}</h3>\n          <p>{`在该教程中，我们将带你从头开发一个简单的存证 Service，并且接入 Muta，进而搭建一条存证链。`}</p>\n          <Jump to=\"../poe-chain/\" mdxType=\"Jump\">Develop A attestation Service</Jump>\n        </li>\n        <li>\n          <h3 {...{\n            \"id\": \"学习开发更为复杂的应用---去中心化交易所\"\n          }}>{`学习开发更为复杂的应用 - 去中心化交易所`}</h3>\n          <Jump to=\"../dex/\" mdxType=\"Jump\">Build a DEX</Jump>\n        </li>\n      </ol>\n    </Step>\n    <h2 {...{\n      \"id\": \"内置-service-说明\"\n    }}>{`内置 Service 说明`}</h2>\n    <p>{`目前 Muta 框架源代码目前内置了四个 built-in Service：Metadata Service、Asset Service、Authorization service 和 Multi-signature service。编译完成，起链之前，对链进行配置的时候。Metadata Service 相关的配置项为起链所必须的配置项。 其他 Service 为可选配置，开发者可以根据实际需求决定是否要在创世块中进行配置。`}</p>\n    <Jump to=\"../../service-list/metadata-service/\" mdxType=\"Jump\">Metadata Service</Jump>\n    <Jump to=\"../../service-list/asset-service/\" mdxType=\"Jump\">Asset Service</Jump>\n    <Jump to=\"../../service-list/auth-service/\" mdxType=\"Jump\">Authorization Service</Jump>\n    <Jump to=\"../../service-list/multi-sig-service/\" mdxType=\"Jump\">Multi-signature Service</Jump>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}