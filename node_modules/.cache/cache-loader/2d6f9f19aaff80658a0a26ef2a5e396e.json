,CAAGW,cAAc,CAAGV,SAAH,CAAe,MAA3C,CAEAR,aAAa,GAEb,mBACE,oBAAC,MAAD,EAAQ,KAAK,CAAEO,KAAf,CAAsB,WAAW,CAAC,kEAAlC,eACE,2BAAK,SAAS,CAAC,qBAAf,eACE,2BAAK,SAAS,CAAC,oBAAf,eACE,yBAAG,IAAI,CAAC,eAAR,CAAwB,KAAK,CAAE,CAACa,KAAK,CAAE,OAAR,CAAiBC,QAAQ,CAAE,OAA3B,CAAoCC,SAAS,CAAE,KAA/C,CAAsDC,UAAU,CAAE,OAAlE,CAA2EC,KAAK,CAAE,MAAlF,CAA/B,eAA0H,yBAAG,SAAS,CAAC,kBAAb,EAA1H,CADF,cAEE,gDAFF,cAGE,+FAAqD,oBAAC,IAAD,EAAM,EAAE,CAAC,kBAAT,4BAArD,KAHF,cAKE,2BAAK,SAAS,CAAC,iBAAf,eACE,oBAAC,GAAD,EAAK,MAAM,CAAE,KAAb,CAAoB,IAAI,CAAC,GAAzB,CAA6B,MAAM,CAAE,IAArC,CAA2C,KAAK,CAAC,WAAjD,EADF,CALF,cASE,yBAAG,SAAS,CAAC,iBAAb,iDATF,cAUE,0CAAG,oBAAC,IAAD,EAAM,EAAE,CAAC,cAAT,eAAwB,yBAAG,SAAS,CAAC,mBAAb,EAAxB,qCAAH,CAVF,CADF,cAaE,2BAAK,SAAS,CAAC,kBAAf,EACGpB,KAAK,CAACqB,GAAN,CAAU,CAAC,CAACC,OAAO,CAAEC,eAAV,CAAD,gBACT,oBAAC,YAAD,EACE,GAAG,CAAEA,eAAe,CAACxB,QAAhB,CAAyBgB,SADhC,CAEE,WAAW,CAAEQ,eAAe,CAACC,WAF/B,CAGE,QAAQ,CAAED,eAAe,CAACxB,QAH5B,CAIE,SAAS,CAAEwB,eAAe,CAACxB,QAAhB,CAAyB0B,SAJtC,eAKE,oBAAC,eAAD,MALF,CADD,CADH,cAUE,oBAAC,iBAAD,EAAmB,QAAQ,CAAE1B,QAA7B,EAVF,CAbF,CADF,CADF,CA8BD,CAED,cAAeF,CAAAA,YAAf","sourcesContent":["import React from 'react';\n\nimport BlogPostItem from '@theme/BlogPostItem';\nimport BlogListPaginator from '@theme/BlogListPaginator';\nimport CTA from '@site/src/components/CTA';\nimport Layout from '@theme/Layout';\nimport Link from '@docusaurus/Link';\n\nimport {enrichTags} from '@site/src/exports/tags';\nimport useDocusaurusContext from '@docusaurus/useDocusaurusContext';\nimport {viewedNewPost} from '@site/src/exports/newPost';\n\nimport './styles.css';\n\nfunction BlogListPage(props) {\n  const {metadata, items} = props;\n  const context = useDocusaurusContext();\n  const {siteConfig = {title: siteTitle}} = context;\n  const {metadata: {post_tags: postTags}} = siteConfig.customFields;\n  const enrichedTags = enrichTags(postTags, 'blog');\n  const typeTags = enrichedTags.filter(tag => tag.category == 'type');\n  const domainTags = enrichedTags.filter(tag => tag.category == 'domain');\n  const isBlogOnlyMode = metadata.permalink === '/';\n  const title = isBlogOnlyMode ? siteTitle : 'Blog';\n\n  viewedNewPost();\n\n  return (\n    <Layout title={title} description=\"Vector blog posts, articles, and tips from the Vector core team.\">\n      <div className=\"blog-list container\">\n        <div className=\"blog-list--filters\">\n          <a href=\"/blog/rss.xml\" style={{float: 'right', fontSize: '1.5em', marginTop: '0px', marginLeft: '-30px', width: '30px'}}><i className=\"feather icon-rss\"></i></a>\n          <h1>The Vector Blog</h1>\n          <p>Thoughts on monitoring and observability from the <Link to=\"/community/#team\">Vector & Timber.io team</Link>.</p>\n\n          <div className=\"margin-vert--lg\">\n            <CTA github={false} size=\"s\" inline={true} style=\"highlight\" />\n          </div>\n\n          <p className=\"margin-vert--sm\">Looking for product updates &amp; announcements?</p>\n          <p><Link to=\"/highlights/\"><i className=\"feather icon-gift\"></i> Check out the highlights section</Link></p>\n        </div>\n        <div className=\"blog-list--items\">\n          {items.map(({content: BlogPostContent}) => (\n            <BlogPostItem\n              key={BlogPostContent.metadata.permalink}\n              frontMatter={BlogPostContent.frontMatter}\n              metadata={BlogPostContent.metadata}\n              truncated={BlogPostContent.metadata.truncated}>\n              <BlogPostContent />\n            </BlogPostItem>\n          ))}\n          <BlogListPaginator metadata={metadata} />\n        </div>\n      </div>\n    </Layout>\n  );\n}\n\nexport default BlogListPage;\n"]}]}