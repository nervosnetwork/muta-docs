AAlB,CACD,CACF,CALD,CAOA,KAAM/B,CAAAA,OAAO,CAAG,EAAhB,CAEA,KAAMgC,CAAAA,YAAY,CAAG,CAACC,IAAD,CAAO1B,MAAP,GAAkB,CACrC,KAAM2B,CAAAA,IAAI,CAAGD,IAAI,CAACE,OAAL,CAAa5B,MAAb,EAAuB,CAApC,CAEA,GAAI,CAAC0B,IAAI,CAACC,IAAD,CAAT,CAAiB,CACfD,IAAI,CAAC,CAAD,CAAJ,CAAQG,KAAR,GACD,CAFD,IAEO,CACLH,IAAI,CAACC,IAAD,CAAJ,CAAWE,KAAX,GACD,CACF,CARD,CAUA,KAAMC,CAAAA,gBAAgB,CAAG,CAACJ,IAAD,CAAO1B,MAAP,GAAkB,CACzC,KAAM+B,CAAAA,IAAI,CAAGL,IAAI,CAACE,OAAL,CAAa5B,MAAb,EAAuB,CAApC,CAEA,GAAI,CAAC0B,IAAI,CAACK,IAAD,CAAT,CAAiB,CACfL,IAAI,CAACA,IAAI,CAACM,MAAL,CAAc,CAAf,CAAJ,CAAsBH,KAAtB,GACD,CAFD,IAEO,CACLH,IAAI,CAACK,IAAD,CAAJ,CAAWF,KAAX,GACD,CACF,CARD,CAUA,KAAMxC,CAAAA,aAAa,CAAG,CAACqC,IAAD,CAAO1B,MAAP,CAAeD,KAAf,GAAyB,CAC7C,OAAQA,KAAK,CAACkC,OAAd,EACE,IAAKpD,CAAAA,IAAI,CAACE,KAAV,CACE0C,YAAY,CAACC,IAAD,CAAO1B,MAAP,CAAZ,CACA,MACF,IAAKnB,CAAAA,IAAI,CAACC,IAAV,CACEgD,gBAAgB,CAACJ,IAAD,CAAO1B,MAAP,CAAhB,CACA,MACF,QACE,MARJ,CAUD,CAXD,CAaAzB,SAAS,CAAC,IAAM,CACd,GAAI,MAAO2D,CAAAA,MAAP,GAAkB,WAAlB,EAAiCA,MAAM,CAACC,QAAxC,EAAoDhB,MAAxD,CAAgE,CAC9D,GAAIiB,CAAAA,QAAQ,CAAGzD,WAAW,CAAC0D,KAAZ,CAAkBH,MAAM,CAACC,QAAP,CAAgBG,MAAlC,CAAf,CAEA,GAAIF,QAAQ,CAACjB,MAAD,CAAZ,CACEG,gBAAgB,CAACc,QAAQ,CAACjB,MAAD,CAAT,CAAhB,CACH,CACF,CAPQ,CAON,EAPM,CAAT,CASA,mBACE,qDACE,2BAAK,SAAS,CAAG,kBAAiBhB,IAAI,EAAI,IAAK,EAA/C,EACGP,KAAK,eAAI,2BAAK,SAAS,CAAC,iBAAf,EAAkCA,KAAlC,CADZ,CAEGL,MAAM,CAACyC,MAAP,CAAgB,CAAhB,GAAsBd,MAAM,cAC3B,oBAAC,cAAD,WACE,mBAAmB,CAAE/B,mBADvB,CAEE,aAAa,CAAEE,aAFjB,CAGE,WAAW,CAAEa,WAHf,CAIE,aAAa,CAAEV,aAJjB,CAKE,IAAI,CAAEW,IALR,CAME,OAAO,CAAEV,OANX,EAOMqB,KAPN,EAD2B,cAS3B,oBAAC,YAAD,WACE,mBAAmB,CAAE3B,mBADvB,CAEE,aAAa,CAAEE,aAFjB,CAGE,aAAa,CAAEG,aAHjB,CAIE,OAAO,CAAEC,OAJX,EAKMqB,KALN,EATD,CAFH,CADF,CAqBItC,QAAQ,CAAC+D,OAAT,CAAiBxB,QAAjB,EAA2ByB,MAA3B,CACEC,KAAK,EAAIA,KAAK,CAAC3B,KAAN,CAAYnB,KAAZ,GAAsBH,aADjC,EAEE,CAFF,CArBJ,CADF,CA4BD,CAED,cAAeqB,CAAAA,IAAf","sourcesContent":["import React, {useState, useEffect, Children} from 'react';\n\nimport Select from 'react-select';\n\nimport classnames from 'classnames';\nimport queryString from 'query-string';\nimport useTabGroupChoiceContext from '@theme/hooks/useTabGroupChoiceContext';\n\nconst keys = {\n  left: 37,\n  right: 39,\n};\n\nfunction ListSwitcher({block, centered, changeSelectedValue, className, handleKeydown, style, values, selectedValue, tabRefs}) {\n  return (\n    <div className={centered ? \"tabs--centered\" : null}>\n      <ul\n        role=\"tablist\"\n        aria-orientation=\"horizontal\"\n        className={classnames('tabs', className, {\n          'tabs--block': block,\n        })}\n        style={style}\n        >\n        {values.map(({value, label}) => (\n          <li\n            role=\"tab\"\n            tabIndex=\"0\"\n            aria-selected={selectedValue === value}\n            className={classnames('tab-item', {\n              'tab-item--active': selectedValue === value,\n            })}\n            key={value}\n            ref={tabControl => tabRefs.push(tabControl)}\n            onKeyDown={event => handleKeydown(tabRefs, event.target, event)}\n            onFocus={() => changeSelectedValue(value)}\n            onClick={() => changeSelectedValue(value)}>\n            {label}\n          </li>\n        ))}\n      </ul>\n    </div>\n  );\n}\nfunction SelectSwitcher({placeholder, selectedValue, changeSelectedValue, size, values}) {\n  let options = values;\n\n  if (options[0].group) {\n    let groupedOptions = _.groupBy(options, 'group');\n\n    options = Object.keys(groupedOptions).map(group => {\n      return {\n        label: group,\n        options: groupedOptions[group]\n      }\n    });\n  }\n\n  return (\n    <Select\n      className={`react-select-container react-select--${size}`}\n      classNamePrefix='react-select'\n      options={options}\n      isClearable={selectedValue}\n      placeholder={placeholder}\n      value={values.find(option => option.value == selectedValue)}\n      onChange={(selectedOption) => changeSelectedValue(selectedOption ? selectedOption.value : null)} />\n  );\n}\n\nfunction Tabs(props) {\n  const {block, centered, children, defaultValue, groupId, label, placeholder, select, size, style, values, urlKey} = props;\n  const {tabGroupChoices, setTabGroupChoices} = useTabGroupChoiceContext();\n  const [selectedValue, setSelectedValue] = useState(defaultValue);\n\n  if (groupId != null) {\n    const relevantTabGroupChoice = tabGroupChoices[groupId];\n    if (\n      relevantTabGroupChoice != null &&\n      relevantTabGroupChoice !== selectedValue\n    ) {\n      setSelectedValue(relevantTabGroupChoice);\n    }\n  }\n\n  const changeSelectedValue = newValue => {\n    setSelectedValue(newValue);\n    if (groupId != null) {\n      setTabGroupChoices(groupId, newValue);\n    }\n  };\n\n  const tabRefs = [];\n\n  const focusNextTab = (tabs, target) => {\n    const next = tabs.indexOf(target) + 1;\n\n    if (!tabs[next]) {\n      tabs[0].focus();\n    } else {\n      tabs[next].focus();\n    }\n  };\n\n  const focusPreviousTab = (tabs, target) => {\n    const prev = tabs.indexOf(target) - 1;\n\n    if (!tabs[prev]) {\n      tabs[tabs.length - 1].focus();\n    } else {\n      tabs[prev].focus();\n    }\n  };\n\n  const handleKeydown = (tabs, target, event) => {\n    switch (event.keyCode) {\n      case keys.right:\n        focusNextTab(tabs, target);\n        break;\n      case keys.left:\n        focusPreviousTab(tabs, target);\n        break;\n      default:\n        break;\n    }\n  };\n\n  useEffect(() => {\n    if (typeof window !== 'undefined' && window.location && urlKey) {\n      let queryObj = queryString.parse(window.location.search);\n\n      if (queryObj[urlKey])\n        setSelectedValue(queryObj[urlKey]);\n    }\n  }, []);\n\n  return (\n    <>\n      <div className={`margin-bottom--${size || 'md'}`}>\n        {label && <div className=\"margin-vert--sm\">{label}</div>}\n        {values.length > 1 && (select ?\n          <SelectSwitcher\n            changeSelectedValue={changeSelectedValue}\n            handleKeydown={handleKeydown}\n            placeholder={placeholder}\n            selectedValue={selectedValue}\n            size={size}\n            tabRefs={tabRefs}\n            {...props} /> :\n          <ListSwitcher\n            changeSelectedValue={changeSelectedValue}\n            handleKeydown={handleKeydown}\n            selectedValue={selectedValue}\n            tabRefs={tabRefs}\n            {...props} />)}\n      </div>\n\n      {\n        Children.toArray(children).filter(\n          child => child.props.value === selectedValue,\n        )[0]\n      }\n    </>\n  );\n}\n\nexport default Tabs;\n"]}]}