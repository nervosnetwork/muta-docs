UAAL,CAtCK,CAuCL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CAvCK,CA0CL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CA1CK,CA6CL,aAAK,QAAL,CA7CK,CAAP,CA+CD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '基本概念',\n\tdescription: 'The fundamental Muta concepts. A great place to start learning about Muta.'\n};\nexport const metadata = {\n  \"id\": \"docs_zh/about/concepts\",\n  \"title\": \"基本概念\",\n  \"description\": \"The fundamental Muta concepts. A great place to start learning about Muta.\",\n  \"source\": \"@site/docs/docs_zh/about/concepts.md\",\n  \"permalink\": \"/new-docs/docs/docs_zh/about/concepts\",\n  \"editUrl\": \"https://github.com/nervosnetwork/muta/edit/master/website/docs/docs_zh/about/concepts.md\",\n  \"sidebar\": \"docs_zh\",\n  \"previous\": {\n    \"title\": \"Muta 介绍\",\n    \"permalink\": \"/new-docs/docs/docs_zh/about/what-is-muta\"\n  },\n  \"next\": {\n    \"title\": \"快速开始\",\n    \"permalink\": \"/new-docs/docs/docs_zh/setup/getting-started\"\n  }\n};\n  /* @jsx mdx */\nimport Jump from '@site/src/components/Jump';\nexport const rightToc = [{\n  value: '基本概念',\n  id: '基本概念',\n  children: [{\n    value: '区块链框架',\n    id: '区块链框架',\n    children: []\n  }, {\n    value: '账户',\n    id: '账户',\n    children: []\n  }, {\n    value: '交易',\n    id: '交易',\n    children: []\n  }, {\n    value: '区块',\n    id: '区块',\n    children: []\n  }, {\n    value: '节点',\n    id: '节点',\n    children: []\n  }, {\n    value: '资产',\n    id: '资产',\n    children: []\n  }, {\n    value: 'Service',\n    id: 'service',\n    children: []\n  }]\n}, {\n  value: '高级概念',\n  id: '高级概念',\n  children: [{\n    value: '多重签名',\n    id: '多重签名',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <p>{`这个文档将带你熟悉一下 Muta 里的一些基本概念，这将有助于你后续的使用和探索。`}</p>\n    <h2 {...{\n      \"id\": \"基本概念\"\n    }}>{`基本概念`}</h2>\n    <h3 {...{\n      \"id\": \"区块链框架\"\n    }}>{`区块链框架`}</h3>\n    <p>{`@yejiayu`}</p>\n    <h3 {...{\n      \"id\": \"账户\"\n    }}>{`账户`}</h3>\n    <p>{`帐户标识了一个能够执行一系列操作的个体。每个用户可以拥有多个帐户。在 Muta 中，账户指定了一对公钥 PubKey 和私钥 PrivKey，PubKey 可以衍生出各种地址，用来标识网络中不同的个体。PrivKey 用于生成数字签名，以证明与 PrivKey 相关联的地址认可某项交易。`}</p>\n    <Jump to=\"/docs/advanced/crypto/\" mdxType=\"Jump\">Muta 中使用到的密码学算法</Jump>\n    <h3 {...{\n      \"id\": \"交易\"\n    }}>{`交易`}</h3>\n    <p>{`交易是一组有序的命令，可以是关于资产的发行、转移，或者合约的部署。交易打包被执行后，链的世界状态将被改变。`}</p>\n    <Jump to=\"/docs/advanced/key-concept/tx\" mdxType=\"Jump\">Muta 中的交易</Jump>\n    <h3 {...{\n      \"id\": \"区块\"\n    }}>{`区块`}</h3>\n    <p>{`区块里存储了交易数据的相关信息，包括交易的内容和账户余额的变化。交易会在 Muta 的共识模块中被打包成块，并进行共识，共识后的区块将在执行模块中被执行。`}</p>\n    <Jump to=\"/docs/advanced/block-structure/\" mdxType=\"Jump\"> Muta 中的区块结构</Jump>\n    <h3 {...{\n      \"id\": \"节点\"\n    }}>{`节点`}</h3>\n    <p>{`区块链网络一般是由多个节点构成，每个节点都运行了 Muta 客户端。它们都拥有整个区块链数据的副本，并通过点对点网络与其他节点进行通信。`}</p>\n    <p>{`由于客户端配置的不同，节点可以分两种：共识节点和同步节点。他们都同步区块链的所有数据，不同之处在于共识节点会参与共识，而同步节点仅负责从监听的共识节点那里同步数据。`}</p>\n    <h3 {...{\n      \"id\": \"资产\"\n    }}>{`资产`}</h3>\n    <p>{`@Eason`}</p>\n    <h3 {...{\n      \"id\": \"service\"\n    }}>{`Service`}</h3>\n    <p>{`@yejiayu`}</p>\n    <h2 {...{\n      \"id\": \"高级概念\"\n    }}>{`高级概念`}</h2>\n    <h3 {...{\n      \"id\": \"多重签名\"\n    }}>{`多重签名`}</h3>\n    <p>{`@Eason`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}